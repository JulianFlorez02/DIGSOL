@page
@model VehiTrans.App.Frontend.VehiculosModel
@{
}
<h1>Registro del Vehiculo</h1>
<br>
<form action="/my-handling-form-page" method="post">
        <p>
            <label for="placa">Placa:</label>
            <input type="text" id="placa" name="placa">
        </p>
        <p>
            <label for="marca">Marca:</label>
            <input type="text" id="marca" name="marca">
        </p>
            <label for="modelo">Modelo:</label>
            <input type="number" id="modelo" name="modelo">
        </p>
        <p>Tipo:
            <input type="radio" name="hm" value="h"> Buseta
            <input type="radio" name="hm" value="m"> Microbus
        </p>
        <p>
            <label for="CantidadPasajeros">Cantidad de Pasajeros: </label>
            <input type="number" id="CantidadPasajeros" name="CantidadPasajeros">
        </p>
        <p>
            <label for="CilindrajeMotor">Cilindraje del Motor: </label>
            <input type="number" id="CilindrajeMotor" name="CilindrajeMotor">
        </p>
        <p>
            <label for="Motor">Motor: </label>
            <input type="number" id="Motor" name="Motor">
        </p>
            <label for="msg">Otros:</label>
            <textarea id="msg" name="user_message"></textarea>
        </p>
        <br>
 
        <p>
            <input type="submit" value="Enviar">
            <input type="reset" value="Borrar">
        </p>

</form>